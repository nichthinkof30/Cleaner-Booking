<h1>Complete Booking</h1>

<div class="field">
  Name:
  <%= @booking.customer_first_name %>, <%= @booking.customer_last_name %>
</div>
<div class="field">
  Cleaner Assigned:
  <%= @booking.cleaner_first_name %>, <%= @booking.cleaner_last_name %>
</div>
<div class="field">
  Booking Schedule Date:
  <%= @booking.date.strftime("%Y-%b-%d") %>
</div>
<div class="field">
  Booking At:
  <%= @booking.created_at.strftime("%Y-%b-%d") %>
</div>